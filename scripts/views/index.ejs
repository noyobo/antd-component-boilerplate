<!DOCTYPE html>
<html>
  <head>
    <meta charset="UTF-8" />
    <title>预览服务</title>
    <link rel="stylesheet" href="//cdn.jsdelivr.net/npm/normalize.css@8.0.1/normalize.min.css" />
    <style>
      body {
        text-align: center;
        font-family: -system-ui, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Helvetica,
          Arial, sans-serif, 'Apple Color Emoji', 'Segoe UI Emoji', 'Segoe UI Symbol';
      }
      .demos {
        padding: 0;
        margin: 0 auto;
        max-width: 400px;
      }
      .demos li {
        font-size: 24px;
        list-style: none;
      }
      .demos li a {
        color: #1092ed;
        display: block;
        text-decoration: none;
      }
      .demos li a:hover {
        color: #1092ed;
        border-bottom: 1px dashed #1092ed;
      }
    </style>
  </head>
  <body>
    <h1><%= componentName %> 预览服务</h1>
    <ul class="demos">
      <% chunks.forEach(function(demo) { %>
      <li><a href="<%= demo[0] %>.html"><%= demo[0] %></a></li>
      <% }); %>
    </ul>
  </body>
</html>
