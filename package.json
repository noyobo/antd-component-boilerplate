  "peerDependencies": {
    "react": "> 16.8.0",
    "react-dom": "> 16.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noyobo/antd-component-boilerplate.git"
  },
  "homepage": "https://github.com/noyobo/antd-component-boilerplate/",
