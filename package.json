    "inquirer": "^6.5.0",
    "jest": "^24.8.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "less-plugin-npm-import": "^2.1.0",
    "lint-staged": ">=9.2.0",
    "merge2": "^1.2.3",
    "mini-css-extract-plugin": "^0.7.0",
    "postcss": "^7.0.17",
    "postcss-loader": "^3.0.0",
