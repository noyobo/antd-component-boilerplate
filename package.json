{
  "name": "@x2s/ppt-templates",
  "private": true,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.com"
  },
  "description": "基于 Antd 组件扩展的组件",
  "author": "noyobo <noyobo@gmail.com>",
  "version": "0.5.7",
  "scripts": {
    "test": "jest -c ./jest.config.js",
    "test:coverage": "jest -c ./jest.config.js --coverage",
    "test:watch": "jest -c ./jest.config.js --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "dist": "node ./scripts/dist.js",
    "preversion": "npm run lint -- --quiet && npm run test",
    "prepublishOnly": "npm run dist",
    "start": "node ./scripts/dev.js",
    "clear": "rm -rf ./es ./lib"
  },
  "files": [
    "lib/",
    "es/"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
