  "license": "MIT",
  "sideEffects": [
    "es/**/style/*",
    "lib/**/style/*",
    "*.less"
  ],
  "pre-push": [],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "**/*.{js,ts,tsx,json,jsx,less}": [
        "prettier --write",
        "git add"
      ],
      "**/*.{js,jsx}": [
        "eslint",
        "git add"
      ],
      "**/*.less": [
        "stylelint --syntax less",
        "git add"
      ]
    }
  }
